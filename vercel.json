{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; connect-src 'self' https://adslider-backend.vercel.app; frame-ancestors 'none'; form-action 'self';"
        },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}